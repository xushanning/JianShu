<resources>

    <style name="wLogin" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">@color/res_99</item>
        <item name="colorControlHighlight">@color/res_99</item>
        <item name="colorControlActivated">@color/res_99</item>
        <item name="colorAccent">@color/res_99</item>
    </style>

    <style name="wSplash" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@color/res_blue</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowTranslucentNavigation">true</item>
    </style>

    <style name="wLoading">
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
</resources>
